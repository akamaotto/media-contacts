# Environment variables for Media Contacts application

# Database connection
DATABASE_URL=your_database_connection_string

# Next.js environment
NEXT_PUBLIC_APP_NAME=Media Contacts
NEXT_PUBLIC_APP_VERSION=1.0.0

# Debug logging (set to true to enable detailed frontend logging)
NEXT_PUBLIC_DEBUG_LOGGING=false

# API configuration
NEXT_PUBLIC_API_BASE_URL=/api

# Feature flags
NEXT_PUBLIC_ENABLE_REGIONS=true
NEXT_PUBLIC_ENABLE_LANGUAGES=true
NEXT_PUBLIC_ENABLE_COUNTRIES=true