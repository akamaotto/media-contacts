{
  "name": "media-contacts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:csv": "jest --testPathPatterns=__tests__/backend/csv",
    "postinstall": "prisma generate",
    "test:e2e": "playwright test --config=config/playwright.config.ts",
    "test:e2e:headed": "playwright test --config=config/playwright.config.ts --headed",
    "test:unit": "jest --config=config/jest.config.js",
    "prisma:seed": "npx prisma db seed",
    "seed:admin": "node prisma/seed-admin.js",
    "seed:media-contacts": "tsx src/lib/seeding/media-contacts/cli.ts",
    "seed:media-contacts:sample": "tsx src/lib/seeding/media-contacts/cli.ts sample",
    "test:e2e:auth": "NODE_ENV=test playwright test tests/auth-flow.spec.ts",
    "test:e2e:setup": "NODE_ENV=test playwright test tests/global-setup.ts",
    "db:push": "npx prisma db push",
    "db:push:prod": "DATABASE_URL=\"$NEON_DATABASE_URL\" npx prisma db push",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:migrate:prod": "DATABASE_URL=\"$NEON_DATABASE_URL\" npx prisma migrate deploy",
    "db:seed:prod": "DATABASE_URL=\"$NEON_DATABASE_URL\" npx prisma db seed",
    "db:admin:prod": "DATABASE_URL=\"$NEON_DATABASE_URL\" node prisma/seed-admin.js",
    "db:backup:local": "pg_dump -U akamaotto -d media_contacts -f ./backups/local_$(date +%Y%m%d%H%M%S).sql",
    "db:backup:prod": "dotenv-cli -e .env.production -- pg_dump -h ep-tight-pine-a2baeagw-pooler.eu-central-1.aws.neon.tech -U neondb_owner -d neondb -f ./backups/prod_$(date +%Y%m%d%H%M%S).sql",
    "db:sync:local-to-prod": "node scripts/db-sync.js local-to-prod",
    "db:sync:prod-to-local": "node scripts/db-sync.js prod-to-local",
    "db:verify-sync": "node scripts/verify-db-sync.js",
    "cache:warm": "tsx scripts/warm-cache.ts",
    "seed:ghana:business:dry": "npx tsx scripts/seeding/ghana-business-seed.ts --dry-run --input scripts/seeding/data/ghana-business-candidates.json",
    "seed:ghana:business:commit": "npx tsx scripts/seeding/ghana-business-seed.ts --commit --input scripts/seeding/data/ghana-business-candidates.json"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
