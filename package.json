{
  "name": "media-contacts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "prisma:seed": "tsx prisma/seed.ts",
    "seed:admin": "node prisma/seed-admin.js",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "db:push": "prisma db push",
    "db:push:prod": "dotenv-cli -e .env.production -- prisma db push",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:prod": "dotenv-cli -e .env.production -- prisma migrate deploy",
    "db:seed:prod": "dotenv-cli -e .env.production -- tsx prisma/seed.ts",
    "db:admin:prod": "dotenv-cli -e .env.production -- node prisma/seed-admin.js",
    "db:backup:local": "pg_dump -U akamaotto -d media_contacts -f ./backups/local_$(date +%Y%m%d%H%M%S).sql",
    "db:backup:prod": "dotenv-cli -e .env.production -- pg_dump -h ep-tight-pine-a2baeagw-pooler.eu-central-1.aws.neon.tech -U neondb_owner -d neondb -f ./backups/prod_$(date +%Y%m%d%H%M%S).sql",
    "db:sync:local-to-prod": "node scripts/db-sync.js local-to-prod",
    "db:sync:prod-to-local": "node scripts/db-sync.js prod-to-local",
    "db:verify-sync": "node scripts/verify-db-sync.js"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@babel/runtime": "^7.27.6",
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.9",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cookie": "^1.0.2",
    "countries-list": "^3.1.1",
    "faker-js": "^1.0.0",
    "lucide-react": "^0.511.0",
    "next": "15.3.3",
    "next-auth": "^5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "nuqs": "^2.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.4",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "vaul": "^1.1.2",
    "zod": "^3.25.42"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.8.0",
    "@playwright/test": "^1.53.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "prisma": "^6.8.2",
    "tailwindcss": "^4",
    "tsx": "^4.19.4",
    "tw-animate-css": "^1.3.2",
    "typescript": "^5"
  }
}
